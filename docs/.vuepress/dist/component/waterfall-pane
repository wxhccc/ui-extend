        <span class="token keyword">this</span><span class="token punctuation">.</span>listData<span class="token punctuation">.</span><span class="token function">push</span><span class="token punctuation">(</span><span class="token operator">...</span>this<span class="token punctuation">.</span><span class="token function">createMockData</span><span class="token punctuation">(</span><span class="token punctuation">)</span><span class="token punctuation">)</span>
